                                <Button Foreground="Black" Content="&#xE002;" Width="50" Height="50" FontFamily="{StaticResource CustomFont}" FontSize="36" Padding="0,0,0,0" Style="{StaticResource DisabledButtonStyle}" Click="HighlightButton_Click" Background="{x:Null}" BorderBrush="{x:Null}"/>
                                <Button Foreground="DimGray" Content="&#xE002;" Width="50" Height="50" FontFamily="{StaticResource CustomFont}" FontSize="36" Padding="0,0,0,0" Style="{StaticResource DisabledButtonStyle}" Click="HighlightButton_Click" Background="{x:Null}" BorderBrush="{x:Null}"/>
                                <Button Foreground="DarkGray" Content="&#xE002;" Width="50" Height="50" FontFamily="{StaticResource CustomFont}" FontSize="36" Padding="0,0,0,0" Style="{StaticResource DisabledButtonStyle}" Click="HighlightButton_Click" Background="{x:Null}" BorderBrush="{x:Null}"/>
                                <Button Foreground="Gray" Content="&#xE002;" Width="50" Height="50" FontFamily="{StaticResource CustomFont}" FontSize="36" Padding="0,0,0,0" Style="{StaticResource DisabledButtonStyle}" Click="HighlightButton_Click" Background="{x:Null}" BorderBrush="{x:Null}"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button Foreground="LightSlateGray" Content="&#xE002;" Width="50" Height="50" FontFamily="{StaticResource CustomFont}" FontSize="36" Padding="0,0,0,0" Style="{StaticResource DisabledButtonStyle}" Click="HighlightButton_Click" Background="{x:Null}" BorderBrush="{x:Null}"/>
                                <Button Foreground="LightGray" Content="&#xE002;" Width="50" Height="50" FontFamily="{StaticResource CustomFont}" FontSize="36" Padding="0,0,0,0" Style="{StaticResource DisabledButtonStyle}" Click="HighlightButton_Click" Background="{x:Null}" BorderBrush="{x:Null}"/>
                                <Button Foreground="White" Content="&#xE002;" Width="50" Height="50" FontFamily="{StaticResource CustomFont}" FontSize="36" Padding="0,0,0,0" Style="{StaticResource DisabledButtonStyle}" Click="HighlightButton_Click" Background="{x:Null}" BorderBrush="{x:Null}"/>
                                <Button Foreground="Transparent" Width="50" Height="50" FontFamily="{StaticResource CustomFont}" FontSize="36" Padding="0,0,0,0" Click="NullHighlightButton_Click" BorderBrush="{x:Null}" Background="#00000000">
                                    <Border BorderBrush="Red" BorderThickness="1" Width="39" RenderTransformOrigin="0.5,0.5">
                                        <Border.RenderTransform>
                                            <CompositeTransform Rotation="46.118"/>
                                        </Border.RenderTransform>
                                    </Border>
                                </Button>
                            </StackPanel>
                        </StackPanel>
                    </Grid>
                    <Custom:ColorPicker x:Name="BackPicker" ColorChanged="BackPicker_ColorChanged" Color="#00000000" Margin="20,0,0,0"/>
                </StackPanel>
            </IviriusWindowingToolkit:DialogWindow>
            <IviriusWindowingToolkit:DialogWindow x:Name="ZippyKeyWindow" FirstButtonText="Apply" SecondButtonText="Get key" DialogAppWindowWidth="450" Title="Zippy API key" HasIcon="True" Icon="/Assets/AppIcons/AppIcon.png" FirstButtonClick="DialogWindow_FirstButtonClick" SecondButtonClick="DialogWindow_SecondButtonClick" d:IsHidden="True">
                <Grid>
                    <TextBlock TextWrapping="Wrap" Text="Enter a valid OpenAI API key" Margin="15,15,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                    <PasswordBox x:Name="ZippyAPIKey" VerticalAlignment="Bottom" Margin="15,0,15,30" PlaceholderText="Current key is hidden for security reasons"/>
                </Grid>
            </IviriusWindowingToolkit:DialogWindow>
        </IviriusWindowingToolkit:ChildAppWindowContainer>
        <IviriusWindowingToolkit:DialogWindowContainer>
            <IviriusWindowingToolkit:DialogWindow x:Name="ActionErrorBox" IsSingleButton="True" Title="Ivirius Text Editor" FirstButtonText="OK" DialogAppWindowWidth="400" RememberPosition="False" PlaySound="True" HasIcon="True" Icon="/Assets/AppIcons/AppIcon.png" Backdrop="MicaAlt" WindowBackground="{StaticResource AppBackgroundBrush}" d:IsHidden="True">
                <Grid>
                    <Image HorizontalAlignment="Left" Height="50" Margin="20,25,0,0" VerticalAlignment="Top" Width="50" Source="/Assets/Glyphs/Error.png"/>
                    <TextBlock x:Name="ActionErrorMessage" TextWrapping="Wrap" FontSize="16" Margin="100,20,25,30"><Run Text="Error code: Unavailable"/><LineBreak/><Run Text="Empty error"/></TextBlock>
                </Grid>
            </IviriusWindowingToolkit:DialogWindow>
            <IviriusWindowingToolkit:DialogWindow x:Name="FileSaveBox" DialogAppWindowHeight="200" DialogAppWindowWidth="420" PlaySound="True" Title="Ivirius Text Editor" RememberPosition="False" HasIcon="True" Icon="/Assets/AppIcons/AppIcon.png" Backdrop="MicaAlt" WindowBackground="{StaticResource AppBackgroundBrush}" d:IsHidden="True">
                <Grid HorizontalAlignment="Left" VerticalAlignment="Top">
                    <Image HorizontalAlignment="Left" Height="75" Margin="15,15,0,0" VerticalAlignment="Top" Width="75" Source="/Assets/Glyphs/Warning.png"/>
                    <TextBlock HorizontalAlignment="Left" Margin="115,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="18" Text="Do you want to save your file?"/>
                    <TextBlock HorizontalAlignment="Left" Margin="115,50,20,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="14"><Run Text="Any changes you made to this file might be "/><LineBreak/><Run Text="lost if you don't save it"/></TextBlock>
                </Grid>
            </IviriusWindowingToolkit:DialogWindow>
            <IviriusWindowingToolkit:DialogWindow x:Name="ActionWarningBox" DialogAppWindowWidth="350" Title="Ivirius Text Editor" PlaySound="True" RememberPosition="False" HasIcon="True" Icon="/Assets/AppIcons/AppIcon.png" Backdrop="MicaAlt" WindowBackground="{StaticResource AppBackgroundBrush}" d:IsHidden="True">
                <Grid MaxWidth="320">
                    <Image HorizontalAlignment="Left" Height="60" Margin="5,0,0,15" VerticalAlignment="Center" Width="60" Source="/Assets/Glyphs/Warning.png"/>
                    <TextBlock x:Name="ActionWarningMessage" TextWrapping="Wrap" FontSize="18" Text="Warning" Margin="85,20,25,0" VerticalAlignment="Top"/>
                </Grid>
            </IviriusWindowingToolkit:DialogWindow>
            <IviriusWindowingToolkit:DialogWindow x:Name="ActionCompleteBox" IsSingleButton="True" Title="Ivirius Text Editor" FirstButtonText="OK" DialogAppWindowWidth="400" RememberPosition="False" PlaySound="True" HasIcon="True" Icon="/Assets/AppIcons/AppIcon.png" Backdrop="MicaAlt" WindowBackground="{StaticResource AppBackgroundBrush}" d:IsHidden="True">
                <Grid>
                    <Image HorizontalAlignment="Left" Height="50" Margin="20,25,0,0" VerticalAlignment="Top" Width="50" Source="/Assets/Glyphs/Check.png"/>
                    <TextBlock x:Name="ActionCompleteMessage" TextWrapping="Wrap" FontSize="16" Margin="100,20,25,30"><Run Text="Error code: Unavailable"/><LineBreak/><Run Text="Empty error"/></TextBlock>
                </Grid>
            </IviriusWindowingToolkit:DialogWindow>
            <IviriusWindowingToolkit:DialogWindow x:Name="FatalExceptionBox" IsSingleButton="True" Title="Ivirius Text Editor" FirstButtonText="OK" DialogAppWindowWidth="400" RememberPosition="False" PlaySound="True" Backdrop="MicaAlt" d:IsHidden="True">
                <Grid>
                    <Image HorizontalAlignment="Left" Height="50" Margin="20,25,0,0" VerticalAlignment="Top" Width="50" Source="/Assets/Glyphs/Error.png"/>
                    <TextBlock x:Name="FatalExceptionMessage" TextWrapping="Wrap" FontSize="16" Margin="100,20,25,30"><Run Text="Error code: Unavailable"/><LineBreak/><Run Text="Empty error"/></TextBlock>
                </Grid>
            </IviriusWindowingToolkit:DialogWindow>
            <IviriusWindowingToolkit:DialogWindow x:Name="PrintingInfoBox" DialogAppWindowWidth="700" Title="Ivirius Text Editor" PlaySound="True" RememberPosition="False" HasIcon="True" Icon="/Assets/Glyphs/Warning.png" Backdrop="MicaAlt" HorizontalContentAlignment="Left" DialogAppWindowHeight="270" IsSingleButton="True" FirstButtonText="Proceed" FirstButtonClick="PrintingInfoBox_FirstButtonClick" d:IsHidden="True">
                <Grid Margin="15,0,15,0">
                    <Image HorizontalAlignment="Left" Height="60" Margin="5,30,0,15" VerticalAlignment="Top" Width="60" Source="/Assets/Glyphs/Warning.png"/>
                    <TextBlock TextWrapping="Wrap" FontSize="18" Text="In order for printing to proceed, you must allow Ivirius Text Editor access to your desktop for creating a temporary bridge file between the app and the printing service. Please press the &quot;Save&quot; and &quot;Select folder&quot; (Please select your desktop on the 2nd dialog) buttons when the file creation dialog appears to allow printing in the future." Margin="85,20,25,0" VerticalAlignment="Top"/>
                </Grid>
            </IviriusWindowingToolkit:DialogWindow>
            <IviriusWindowingToolkit:DialogWindow x:Name="NewFileBox" DialogAppWindowWidth="350" Title="Ivirius Text Editor" PlaySound="True" RememberPosition="False" HasIcon="True" Icon="/Assets/AppIcons/AppIcon.png" Backdrop="MicaAlt" WindowBackground="{StaticResource AppBackgroundBrush}" d:IsHidden="True" FirstButtonClick="NewFileBox_FirstButtonClick">
                <Grid MaxWidth="320">
                    <Image HorizontalAlignment="Left" Height="60" Margin="5,0,0,15" VerticalAlignment="Center" Width="60" Source="/Assets/Glyphs/Warning.png"/>
                    <TextBlock x:Name="ActionWarningMessage1" TextWrapping="Wrap" FontSize="18" Text="Do you want to save your current file before creating a new one?" Margin="85,20,25,0" VerticalAlignment="Top"/>
                </Grid>
            </IviriusWindowingToolkit:DialogWindow>
        </IviriusWindowingToolkit:DialogWindowContainer>
        <Grid x:Name="LoadingScreen" Background="{StaticResource AppBackgroundBrush}" d:IsHidden="True">
            <Image Height="100" Width="100" Margin="0,0,0,50" Source="/Assets/AppIcons/AppIcon.png"/>
            <Custom:ProgressRing IsActive="True" Width="25" Height="25" Margin="0,175,0,0"/>
        </Grid>
        <ContentDialog x:Name="ChangelogWindow" Title="Changelog - Version 3.1.0.113" PrimaryButtonText="Close" Background="{StaticResource AppTitleBarBrush}" d:IsHidden="True">
            <Grid Margin="-24,-63,-24,-105" Background="{StaticResource AppTitleBarBrush}">
                <Grid Background="{StaticResource AppBackgroundBrush}"/>
                <TextBlock TextWrapping="Wrap" Text="Changelog - Version 3.1.0.113" HorizontalAlignment="Left" VerticalAlignment="Top" Style="{ThemeResource SubtitleTextBlockStyle}" Margin="25,24,0,0"/>
                <StackPanel Margin="24,63,24,105">
                    <TextBlock TextWrapping="Wrap"><Run Text="New features:"/><LineBreak/><Run Text="- Add Template support"/><LineBreak/><Run Text="- Fix several bugs with opening templates in the Home Menu"/><LineBreak/><Run Text="- Add a Home Menu shortcut in the File menu"/><LineBreak/></TextBlock>
                </StackPanel>
            </Grid>
        </ContentDialog>
    </Grid>
